{
  "name": "dash-commons",
  "version": "1.0.0",
  "description": "use the dash-commons package as a combined package of lodash-underscore in one object",
  "main": "index.js",
  "exports": {
    ".": {
      "require": "./index.js",
      "import": "./index.mjs"
    }
  },
  "directories": {
    "test": "test"
  },
  "pre-commit": {
    "run": [
      "test"
    ]
  },
  "pre-push": {
    "run": [],
    "silent": true
  },
  "scripts": {
    "test": "mocha --reporter spec --recursive --timeout 60000"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "underscore": "^1.13.6"
  },
  "devDependencies": {
    "@types/node": "^20.11.17",
    "chai": "^4.4.1",
    "clean-pkg-json": "^1.2.0",
    "esbuild": "^0.20.0",
    "mocha": "^10.2.0",
    "pre-commit": "^1.2.2",
    "pre-push": "^0.1.4",
    "sinon": "^17.0.1",
    "typescript": "^5.3.3",
    "unimported": "^1.31.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ganeshkbhat/lodash-underscore.git"
  },
  "bugs": {
    "url": "https://github.com/ganeshkbhat/lodash-underscore/issues"
  },
  "homepage": "https://github.com/ganeshkbhat/lodash-underscore#readme",
  "author": "Ganesh Bhat <ganeshsurfs@gmail.com>",
  "license": "MIT"
}